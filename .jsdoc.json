    "include": [
      "src/client",
      "src/server",
      "src/common"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "projectTitle": "Soundworks",
    "destination": "./docs",
    "encoding": "utf8",
    "template": "./node_modules/docdash"
  },
  "templates": {
